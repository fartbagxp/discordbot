{
  "name": "discordbot",
  "version": "1.0.0",
  "description": "bot for discord",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "cover": "istanbul cover --report html --report cobertura node_modules/mocha/bin/_mocha -- -R spec --recursive src/**/*.test.js",
    "lint": "eslint --ignore-pattern public --ignore-pattern api-docs --ignore-pattern coverage .",
    "test": "mocha --reporter spec src/**/*.test.js"
  },
  "repository": {
