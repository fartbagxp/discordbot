  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fartbagxp/discordbot/issues"
  },
  "homepage": "https://github.com/fartbagxp/discordbot#readme",
  "dependencies": {
    "bluebird": "3.5.0",
    "debug": "2.6.8",
    "discord.js": "11.1.0",
    "dotenv": "4.0.0",
    "node-opus": "0.2.6",
    "redis": "2.7.1"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2"
  }
