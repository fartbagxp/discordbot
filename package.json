  "dependencies": {
    "bluebird": "3.5.1",
    "debug": "3.1.0",
    "discord.js": "11.2.1",
    "dotenv": "4.0.0",
    "lodash": "4.17.4",
    "node-opus": "0.2.7",
    "redis": "2.8.0"
  },
  "devDependencies": {
    "eslint": "^4.11.0",
