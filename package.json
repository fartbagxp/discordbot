{
  "name": "discordbot",
  "version": "1.0.0",
  "description": "bot for discord",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "cover":
      "istanbul cover --report html --report cobertura node_modules/mocha/bin/_mocha -- -R spec --recursive src/**/*.test.js",
    "lint":
      "eslint --ignore-pattern public --ignore-pattern api-docs --ignore-pattern coverage .",
    "test": "mocha --reporter spec src/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fartbagxp/discordbot.git"
  },
  "keywords": ["discord"],
  "author": "fartbagxp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fartbagxp/discordbot/issues"
  },
  "homepage": "https://github.com/fartbagxp/discordbot#readme",
  "dependencies": {
    "bluebird": "3.5.1",
    "debug": "3.1.0",
    "discord.js": "11.3.0",
    "dotenv": "5.0.1",
    "lodash": "4.17.5",
    "node-opus": "0.2.7",
    "redis": "2.8.0"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "prettier": "^1.11.1"
  }
}
